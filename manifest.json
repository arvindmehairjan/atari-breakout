{
    "name": "Breakout Game",
    "version": "1.0",
    "description": "A clone of the 1976-classic game Breakout made by Atari",
    "manifest_version": 2,
    "icons": {"128": "icon_128.png"},
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },
    "permissions": ["activeTab"]
}